{
  "name": "@360mediadirect/fullstack-serverless",
  "version": "0.8.7",
  "engines": {
    "node": ">=4.0"
  },
  "description": "Deploy a fullstack serverless web app (including React, Angular, Node.js, Python) with an AWS CloudFront distribution in front of your API Gateway and static content from S3. Use your own custom domain, SSL from AWS, and AWS Web Application Firewall.",
  "author": "360MediaDirect",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/360MediaDirect/fullstack-serverless"
  },
  "keywords": [
    "serverless",
    "fullstack",
    "full stack",
    "serverless plugin",
    "api gateway",
    "cloudfront",
    "s3",
    "cloudformation",
    "lambda",
    "aws",
    "aws lambda",
    "amazon web services",
    "serverless.com"
  ],
  "main": "index.js",
  "dependencies": {
    "async": "^3.2.5",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.1",
    "lodash": "^4.17.21",
    "mime": "^2.6.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "sinon": "^17.0.1",
    "@types/jest": "^29.5.11"
  },
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "index.js",
      "lib/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/?(*.)+(spec|test).js"
    ]
  }
}
